{
  "name": "nextjs-cloudflare-pages-d1-drizzle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:seed:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) tsx ./db/seed.ts",
    "db:migrate:local": "wrangler d1 migrations apply my-database-name --local",
    "db:migrate:preview": "wrangler d1 migrations apply --env preview my-preview-database-name --remote",
    "db:migrate:prod": "wrangler d1 migrations apply my-database-name --remote",
    "db:studio:local": "cross-env DB_LOCAL_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:preview": "cross-env NODE_ENV='preview' drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "pages:build": "pnpm next-on-pages",
    "dev": "next dev",
    "build": "next build",
    "pages:dev": "pnpm pages:build && pnpm wrangler pages dev",
    "pages:deploy:preview": "pnpm pages:build && pnpm wrangler pages deploy --branch preview",
    "pages:deploy": "pnpm pages:build && pnpm wrangler pages deploy"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@notionhq/client": "^2.2.15",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-hookz/web": "^24.0.4",
    "@tanstack/react-query": "^5.50.1",
    "@tanstack/react-query-next-experimental": "^5.50.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "feed": "^4.2.2",
    "lucide-react": "^0.400.0",
    "next": "14.2.4",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "react-layout-masonry": "^1.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.12.1",
    "@cloudflare/workers-types": "^4.20240712.0",
    "@haydenull/fabric": "^0.2.6",
    "@next/env": "^14.2.5",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query-devtools": "^5.50.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "better-sqlite3": "^11.1.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5",
    "unplugin-turbo-console": "^1.9.11",
    "wrangler": "^3.64.0"
  }
}